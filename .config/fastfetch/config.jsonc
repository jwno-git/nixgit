{
  "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
  "modules": [ 
    "break",
  // Title
     {
      "type": "title",
      "format": "{#cyan}╭ {#}{user-name-colored}{#cyan} ──────────"
    },
    {
      "type": "host",
      "key": "{#cyan}│ {#keys}Host"
    },
    {
      "type": "kernel",
      "key": "{#cyan}│ {#keys}Kernel"
    },
    {
      "type": "os",
      "key": "{#cyan}│ {#keys}Os"
    },
    {
      "type": "packages",
      "key": "{#cyan}│ {#keys}Packages"
    },
    //{
    //  "type": "title",
    //  "format": "{#cyan} ╰─────────────────────────"
    //},
    //{
    //  "type": "title",
    //  "format": "{#cyan} ╭─────────────────────────"
    //},
    {
      "type": "cpu",
      "temp": true,
      "key": "{#cyan}│ {#keys}CPU"
    },
    {
      "type": "cpuusage",
      "key": "{#cyan}│ {#keys}Cpuusage"
    },
    {
      "type": "processes",
      "key": "{#cyan}│ {#keys}Processes"
    },
    {
      "type": "disk",
      "key": "{#cyan}│ {#keys}Disk"
    },
    {
      "type": "swap",
      "key": "{#cyan}│ {#keys}Swap"
    },
    {
      "type": "memory",
      "key": "{#cyan}│ {#keys}Memory"
    },
    {
      "type": "title",
      "format": "{#cyan}╰───────────────────────────────────────────"
    },
    "break"
  ]
}
